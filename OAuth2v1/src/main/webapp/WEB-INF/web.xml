<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Demo Webapp for Google OAuth2</display-name>
  <servlet>
  	<servlet-name>Testing</servlet-name>
  	<display-name>Testing</display-name>
  	<description></description>
  	<servlet-class>com.danter.google.auth.Testing</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PostTesting</servlet-name>
  	<display-name>PostTesting</display-name>
  	<description></description>
  	<servlet-class>gosigma.tomcat.PostTesting</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Testing</servlet-name>
  	<url-pattern>/Testing</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PostTesting</servlet-name>
  	<url-pattern>/PostTesting</url-pattern>
  </servlet-mapping>
</web-app>
